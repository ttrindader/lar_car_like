      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.175621 0.225891 0.108579"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="10.768651"/>
